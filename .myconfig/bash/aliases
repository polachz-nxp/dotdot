alias lla='ls -Al'
alias gh='history|grep'
alias left='ls -t -1'
alias count='find . -type f | wc -l'
alias lt='ls --human-readable --size -1 -S --classify'
alias ..='cd ..'
alias ...='cd ../..'
alias makeme='sudo chown $USER:$USER'
alias makeroot='sudo chown 0:0'
alias hh='cd ~'
alias ee='cd /etc'

